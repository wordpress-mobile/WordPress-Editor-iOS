  pod 'WordPress-iOS-Editor', :path => '../'
  pod 'CocoaLumberjack', '~>2.0.0', :inhibit_warnings => true
  pod 'WordPress-iOS-Shared', '~>0.5.0', :inhibit_warnings => true
  pod 'WordPressCom-Analytics-iOS', '~>0.0.40', :inhibit_warnings => true
end

